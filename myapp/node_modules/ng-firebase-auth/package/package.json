{
  "name": "ng-firebase-auth",
  "description": "A directive for easily adding Firebase authentication to AngularJS App",
  "version": "0.0.1",
  "author": "CJ Ponti <cjponti@gmail.com>",
  "browser": {
    "angular": "./node_modules/angular/angular.js",
    "angular-cookies": "./node_modules/angular-cookies/angular-cookies.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": {
      "depends": "jquery:jQuery",
      "exports": "angular"
    },
    "angular-cookies": {
      "depends": "angular",
      "exports": "angular.module('ngCookies').name"
    },
    "angular-ui-router": {
      "depends": "angular"
    }
  },
  "bugs": {
    "url": "https://github.com/theponti/ng-firebase-auth/issues"
  },
  "devDependencies": {
    "angular": "1.3.5",
    "angular-cookies": "1.3.5",
    "angular-mocks": "^1.3.8",
    "angular-route": "1.3.5",
    "angularfire": "^0.9.0",
    "bower": "^1.3.12",
    "browserify": "^7.0.3",
    "browserify-ngannotate": "^0.5.0",
    "browserify-shim": "^3.8.2",
    "gulp": "^3.8.6",
    "gulp-concat": "^2.2.0",
    "gulp-if": "^1.2.5",
    "gulp-jshint": "~1.5.5",
    "gulp-karma": "0.0.4",
    "gulp-load-plugins": "^0.8.0",
    "gulp-minify-css": "^0.3.11",
    "gulp-minify-html": "^0.1.7",
    "gulp-protractor": "0.0.12",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^1.2.4",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.0.2",
    "jasmine-core": "^2.1.2",
    "jquery": "^2.1.1",
    "jshint-stylish": "~0.1.5",
    "karma": "^0.12.28",
    "karma-browserify": "^2.0.0",
    "karma-chrome-launcher": "^0.1.5",
    "karma-jasmine": "^0.3.1",
    "protractor": "^1.5.0",
    "run-sequence": "^1.0.2",
    "superstatic": "^1.1.7",
    "vinyl-source-stream": "^1.0.0"
  },
  "engines": {
    "node": "0.10.35"
  },
  "homepage": "https://github.com/theponti/ng-firebase-auth",
  "keywords": [
    "static",
    "node",
    "html",
    "css",
    "sass",
    "angular"
  ],
  "license": "MIT",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/theponti/ng-firebase-auth.git"
  },
  "scripts": {
    "bootstrap": "npm install && bower install",
    "build": "gulp build",
    "build-prod": "gulp prod",
    "dev": "gulp",
    "postinstall": "bower install && gulp build",
    "protractor": "gulp protractor",
    "start": "gulp prod && gulp server",
    "test": "node_modules/karma/bin/karma start test/karma.conf.js",
    "test-single": "node_modules/karma/bin/karma start test/karma.conf.js --single-run"
  }
}
